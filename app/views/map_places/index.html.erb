<h1>Listing map_places</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @map_places.each do |map_place| %>
      <tr>
        <td><%= map_place.place %></td>
        <td><%= map_place.latitude %></td>
        <td><%= map_place.longitude %></td>
        <td><%= link_to 'Show', map_place %></td>
        <td><%= link_to 'Edit', edit_map_place_path(map_place) %></td>
        <td><%= link_to 'Destroy', map_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Map place', new_map_place_path %>
